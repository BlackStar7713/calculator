<?xml version="1.0" encoding="utf-8"?>
<resources>
    <array name="faq_functions_examples">
        <!--TRIGONOMETRIC-->
        <item>
            sin(x) - синус x
            \nПример: \'sin(45)\'
        </item>
        <item>
            cos(x) - косинус x
            \nПример: \'cos(45)\'
        </item>
        <item>
            tan(x) - тангенс x
            \nПример: \'tan(45)\'
        </item>
        <item>
            cot(x) - котангенс x
            \nПример: \'cot(45)\'
        </item>
        <item>
            asin(x) - арксинус x
            \nРезультат лежит в пределах [-pi/2, pi/2](или [-90, 90], в зависимости от режима)
            \nПример: \'asin(0.5)\'
        </item>
        <item>
            acos(x) - арккосинус x
            \nРезультат лежит в пределах [0, pi](или [0, 180], в зависимости от режима)
            \nПример: \'acos(0.5)\'
        </item>
        <item>
            atan(x) - арктангенс x
            \nРезультат лежит в пределах (-pi/2, pi/2)(или (-90, 90), в зависимости от режима)
            \nПример: \'atan(33)\'
        </item>
        <item>
            acot(x) - арккотангенс x
            \nРезультат лежит в пределах (0, pi)(или (0, 180), в зависимости от режима)
            \nПример: \'acot(33)\'
        </item>
        <item>
            sinh(x) - гиперболический синус x
            \nПример: \'sinh(25)\'
        </item>
        <item>
            cosh(x) - гиперболический косинус x
            \nПример: \'cosh(25)\'
        </item>
        <item>
            tanh(x) - гиперболический тангенс x
            \nПример: \'tanh(32)\'
        </item>
        <item>
            coth(x) - гиперболический котангенс x
            \nПример: \'coth(32)\'
        </item>
        <!--TRIGONOMETRIC-->

        <!--POW-->
        <item>
            pow(x, y) - x в степени y
            \nПример: \'pow(3, 2)\'
        </item>
        <item>
            sqrt(x) - положительный корень квадратный x
            \nПример: \'sqrt(2)\'
        </item>
        <item>
            exp(x) - e в степени x
            \nПример: \'exp(2)\'
        </item>
        <!--POW-->

        <!--LOG-->
        <item>
            log(x, y) - логарифм y по основанию x
            \nПример: \'log(3, 9)\'
        </item>
        <item>
            ln(x) - натуральный логарифм x
            \nПример: \'ln(5)\'
        </item>
        <item>
            log10(x) - логарифм x по основанию 10
            \nПример: \'log10(5)\'
        </item>
        <!--LOG-->

        <!--MATH-->
        <item>
            abs(x) - модуль x
            \nПример: \'abs(-2)\'
        </item>
        <item>
            floor(x) - ближайшее целое меньше или равное x
            \nПример: \'floor(23.3)\'
        </item>
        <item>
            ceil(x) - ближайшее целое больше или равное x
            \nПример: \'ceil(23.3)\'
        </item>
        <item>
            min(x, y) - минимальное из x и y
            \nПример: \'min(3, 4)\'
        </item>
        <item>
            max(x, y) - наибольшее из x и y
            \nПример: \'max(5, 6)\'
        </item>
        <item>
            random() - псевдо-случайное число из интервала [0, 1)
            \nПример: \'random()\'
        </item>
        <item>
            round(x) - ближайшее к x целое
            \nПример: \'round(5.5)\'
        </item>
        <item>
            signum(x) - 0 если x = 0; 1 если x больше 0; -1 если x меньше 0
            \nПример: \'signum(-2)\'
        </item>
        <item>
            fact(x) - факториал x, где x положительное целое
            \nПример: \'fact(5)\'
        </item>
        <item>
            mod(x, y) - остаток деления x на y, результат неотрицательный
            \nПример: \'mod(7, 5)\'
        </item>
        <!--MATH-->
    </array>
